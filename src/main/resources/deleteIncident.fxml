         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Incident">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <ComboBox fx:id="IncidentToDelete" prefWidth="150.0">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </ComboBox>
            <Button fx:id="DeleteIncident" mnemonicParsing="false" onAction="#DeleteIncident" text="Delete">
               <VBox.margin>
