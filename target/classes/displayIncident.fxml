<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controles.displayIncident">
   <children>
      <Label layoutX="264.0" layoutY="14.0" text="Incidents">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TableView fx:id="IncidentTab" layoutX="26.0" layoutY="60.0" prefHeight="267.0" prefWidth="549.0">
         <columns>
            <TableColumn fx:id="DescriptionColumn" prefWidth="192.0" text="Description" />
            <TableColumn fx:id="HourColumn" minWidth="0.0" prefWidth="103.0" text="Hour" />
            <TableColumn fx:id="PlaceColumn" minWidth="0.0" prefWidth="135.0" text="Place" />
            <TableColumn fx:id="typeColumn" minWidth="0.0" prefWidth="118.0" text="Type" />
         </columns>
      </TableView>
      <Button fx:id="AddButton" layoutX="196.0" layoutY="339.0" mnemonicParsing="false" onAction="#AddButton" text="ADD" />
      <Button fx:id="DeleteButton" layoutX="345.0" layoutY="339.0" mnemonicParsing="false" onAction="#DeleteButton" text="Delete" />
      <Button fx:id="injury" layoutX="517.0" layoutY="12.0" mnemonicParsing="false" text="Injury" />
   </children>
</AnchorPane>
